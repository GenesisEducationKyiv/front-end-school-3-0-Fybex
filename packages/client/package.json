  "name": "@music-app/client",
  "private": true,
  "scripts": {
    "build": "tsc -b && vite build",
    "build-storybook": "storybook build",
    "build:analyze": "ANALYZE=true npm run build",
    "dev": "vite",
    "dev:with-proto": "npm run proto:generate:build && npm run dev",
    "generate:api": "openapi-zod-client \"${OPENAPI_URL:-http://localhost:8000/documentation/yaml}\" -o \"./src/generated/api.ts\" --export-schemas",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "proto:generate:build": "npm run generate:build --workspace=@music-app/proto",
    "storybook": "storybook dev -p 6006",
    "test:ct": "playwright test -c playwright-ct.config.ts",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:full": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:integration": "npm run proto:generate:build && vitest --project integration",
    "test:unit": "npm run proto:generate:build && vitest --project unit",
    "test:vitest": "npm run proto:generate:build && vitest"
  },
  "type": "module",
  "version": "0.0.0",
  "msw": {
    "workerDirectory": [
      "packages/client/public"
    ]
  }
}
